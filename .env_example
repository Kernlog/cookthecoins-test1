# Port for the server to listen on (Render will set this automatically)
PORT=3000

# Solana network to connect to (devnet or mainnet-beta)
SOLANA_NETWORK=devnet

# Your wallet's private key (Base64 encoded) - KEEP THIS SECRET!
# This is the wallet that will be sending tokens to users
AIRDROP_WALLET_PRIVATE_KEY=your_private_key_here